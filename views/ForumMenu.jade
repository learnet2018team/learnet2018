// Sidenav Forum register
.sidenavforum
    .container.sidenavf-header
        .close-navforum
            i.fas.fa-times
    .container
        .form-area
            form(role='form')
                br(style='clear:both')
                h3(style='color: white; margin-bottom: 25px; text-align: center;') Crea un nuevo Foro!
                .form-group
                    input#name.form-control(type='text' name='nombre' placeholder='Nombre' required='')
                .form-group
                    input#email.form-control(type='text' name='correo' placeholder='Correo electronico' required='')
                .form-group
                    input#mobile.form-control(type='text' name='tematica' placeholder='Tematica' required='')
                .form-group
                    input#subject.form-control(type='text' name='asunto' placeholder='Asunto' required='')
                .form-group
                    textarea#cuerpo.form-control(type='textarea' placeholder='Cuerpo del foro' maxlength='500' rows='7')
                    span.help-block
                        p#characterLeft.help-block Has superado el limite de caracteres
                button#submit.btn.btn-primary.pull-right(type='button' name='submit') Subir Foro
